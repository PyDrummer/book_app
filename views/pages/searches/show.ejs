<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../../layout/head') %>
</head>
<body>
  <main class="container">
    <header>
      <%- include('../../layout/header') %>
    </header>

    <h2>Search Results</h2>
    <ul>
      <% bookInfo.forEach(book => { %>
      <article class="new_book">
      <img src="<%= book.bookPic %>" />
      <h3> <%= book.bookTitle %> </h3>
      <p id="book_author"> <%= book.bookAuthor %> </p>
      <p id="book_desc"> <%= book.description %> </p>
      <p><%= book.isbn %> </p>
      <a id="save" href="/save/<%= book.isbn %>">Save Book!</a>
      </article> 
      <% }) %>
    </ul>
  </main>
</body>
</html>


